function fizzbuzz(num) {
    for (var i = 1; i <= num; i = i + 1) {
        if (i % 15 == 0) {
            print 'fizzbuzz';
        } else if (i % 3 == 0) {
            print 'fizz';
        } else if (i % 5 == 0) {
            print 'buzz';
        } else {
            print i;
        }
    }
}

fizzbuzz(10);
